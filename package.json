{
  "name": "@daostack/test-env",
  "version": "3.0.0",
  "description": "Testing environment for DAOstack projects.",
  "main": "index.js",
  "scripts": {
    "migrate": "node ./migrate.js",
    "release": "./release.sh",
    "prepare-subgraph": "npm explore @daostack/subgraph -- npm i && node setup-subgraph.js",
    "deploy-subgraph": "npm run prepare-subgraph && npm explore @daostack/subgraph -- npm run deploy",
    "reset-migration": "cp -f node_modules/@daostack/migration/migration.json ./migration.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daostack/test_env.git"
  },
  "keywords": [
    "DAOstack"
  ],
  "author": "DAOstack (https://www.daostack.io)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/daostack/test_env/issues"
  },
  "homepage": "https://github.com/daostack/test_env#readme",
  "dependencies": {
    "@daostack/arc": "0.0.1-rc.24",
    "@daostack/migration": "0.0.1-rc.24-v1",
    "@daostack/subgraph": "0.0.27-1",
    "ipfs-http-client": "^32.0.1"
  }
}
